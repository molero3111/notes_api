apiVersion: v1
kind: ConfigMap
metadata:
  name: chat-config
data:
  DEBUG: "True"
  ALLOWED_HOSTS: "*"
  CSRF_TRUSTED_ORIGINS: "http://* https://*"
  CORS_ALLOWED_ORIGINS: "ALL"
  DJANGO_SETTINGS_MODULE: "notes_manager.settings"
  POSTGRES_HOST: "notes-db"
  POSTGRES_PORT: "5432"
  POSTGRES_USER: "postgres"
  POSTGRES_DB: "notes_db"
  REDIS_HOST: "notes-redis"
  REDIS_PORT: "6379"
  REDIS_APP_DB: "1"
  REDIS_DEFAULT_DB: "0"
  REDIS_DEFAULT_KEY_EXPIRATION: "3600"
  URL_PREFIX: "notes-crud"